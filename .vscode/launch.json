    "version": "0.2.0",
    "configurations": [
        {
            "name": "Build proto",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/infra/vprotogen",
            "env": {},
            "args": ["-repo=${workspaceFolder}"]

        },
        
        
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main",
            "env": {},
            "args": ["-format=v2board"]
        }
    ]
}